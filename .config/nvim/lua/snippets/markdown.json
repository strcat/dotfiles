{
  "MkDocsAdmonition": {
    "prefix": "mkdocs-admonition",
    "body": [
      "!!! ${1|summary,info,tip,success,question,warning,failure,danger,bug,example,quote|} \"${2}\"",
      "",
      "    ${0}"
    ],
    "description": "Add admonition to document"
  },
  "Admonition": {
    "prefix": "admonition",
    "body": [
      ":::${1|note,tip,info,caution,danger|} ${2}",
      "",
      "${0}",
      "",
      ":::"
    ],
    "description": "Add admonition to document"
  },
  "Blockquote": {
          "prefix": "pblock",
          "body": [ ">>>",
                  "${1}",
                  "<cite class=\"cblock\">${2}<cite>",
                  ">>>",
                  "${0}"],
                  "description": "Blockquote for Press"
  },
  "Content tabs": {
    "prefix": "contenttabs",
    "body": [
      "===! \"${1}\"",
      "",
      "    ${2}",
      "",
      "=== \"${3}\"",
      "",
      "    ${0}"
    ],
    "description": "Add content tabs to document"
  },
  "Prettier ignore": {
    "prefix": "prettier-ignore",
    "body": ["<!-- prettier-ignore -->", "${0}"],
    "description": "Add prettier-ignore mark to document"
  },
  "Test": {
          "prefix": "bcite",
          "body": ["<cite class=\"cblock\">${0}<cite>"],
          "description": "Linkclass for Links in Blockquote"
  },
  "Cite source": {
    "prefix": "cite",
    "body": ["-- <cite>${0}</cite>"],
    "description": "Add cited source to document"
  },
  "Sec": {
    "prefix": "sec",
    "description": "Section",
    "body": ["# ${1:Section Name} #", "$0"]
  },
  "ssec": {
    "prefix": "ssec",
    "description": "Sub Section",
    "body": ["## ${1:Section Name} ##", "$0"]
  },
  "Insert code block": {
    "prefix": "cbl",
    "body": [
      "```${1:language}",
      "$2",
      "```",
      "$0"
    ],
    "description": "Insert fenced code block"
  },
  "fnt": {
    "prefix": "fnt",
    "description": "Footnote",
    "body": [
      "[^${1:${TM_SELECTED_TEXT:Footnote}}]$0",
      "",
      "[^$1]:${2:Text}"
    ]
  },
  "ssec": {
    "prefix": "ssec",
    "description": "Sub Section",
    "body": ["## ${1:Section Name} ##", "$0"]
  },
  "sssec": {
    "prefix": "sssec",
    "description": "SubSub Section",
    "body": "### ${1:Section Name} ### $0"
  },
  "par": {
    "prefix": "par",
    "description": "Paragraph",
    "body": ["#### ${1:Paragraph Name} ####", "$0"]
  },
  "spar": {
    "prefix": "spar",
    "description": "Paragraph",
    "body": ["##### ${1:Paragraph Name} #####", "$0"]
  },
  "ilc": {
    "prefix": "ilc",
    "description": "Inline Code",
    "body": "`$1`$0"
  },
  "link": {
    "prefix": "link",
    "description": "link",
    "body": "[$1]($2)"
  },
  "table": {
    "prefix": "table",
    "description": "gfm table",
    "body": [
      "| ${1:...}                  | ${2:...}                  |",
      "| ------------------------- | ------------------------- |",
      "| ${3:...}                  | ${4:...}                  |"
    ]
  },
  "keyboard tag": {
    "prefix": "kbd",
    "body": "<kbd>${1}</kbd> ${0}",
    "description": "Add an HTML keyboard tag"
  },
  "hugo frontmatter": {
    "prefix": "hugofmt",
    "body": [
      "---",
      "title: '${1:$TM_FILENAME_BASE}'",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}",
      "---",
      "${0}"
    ],
    "description": "Add hugo TOML frontmatter"
  },
  "Comment": {
    "prefix": "<!",
    "body": "<!-- ${1} -->${0}",
    "description": "Add a comment span"
  },
  "collapse": {
    "prefix": "collapse",
    "body": [
      "<details>",
      "    <summary markdown=\"span\">${1}</summary>",
      "</details>"
    ],
    "description": "Create a collapsible content"
  },
  "Katex Block": {
    "prefix": "$$",
    "body": "\\$\\$${1}\\$\\$${0}",
    "description": "Add a Katex block"
  },
  "Katex Inline": {
    "prefix": "katex",
    "body": "\\\\\\\\(${1}\\\\\\\\)${0}",
    "description": "Add a Katex inline block"
  },
  "Link Killercoda": {
    "prefix": "linkkc",
    "scope": "markdown",
    "body": ["[${1:Link text}]({{TRAFFIC_HOST1_${2:PORT}}})${0}"],
    "description": "Link to Killercoda"
  },
  "Details HTML": {
    "prefix": "det",
    "scope": "markdown",
    "body": [
      "### ${1:TASK_DETAILS}",
      "",
      "<details>",
      "<summary>click to see the answer</summary>",
      "<code>${2:ANSWER}</code>",
      "</details>${0}"
    ],
    "description": "Details HTML snippet"
  },
  "Highlight text to make it stand out on the page": {
    "prefix": "diff",
    "scope": "markdown",
    "body": ["```diff", "! ${1:TEXT} !", "```${0}"],
    "description": "Highlight markdown text as important, it will render differently depending on the diff highlighting tooling"
  },
  "Markdown reference href part": {
    "prefix": "refh",
    "scope": "markdown",
    "body": ["<a id=\"${1}\" href=\"$CLIPBOARD\">[[${1}]]</a> : ${3:desc}"],
    "description": "Insert href reference at the bottom"
  },
  "Markdown reference": {
    "prefix": "ref",
    "scope": "markdown",
    "body": ["[[${1}]](#${1})"],
    "description": "Insert reference to something"
  },
  "YAML Code Snippet": {
    "prefix": "yml",
    "scope": "markdown",
    "body": ["```yaml", "${1}", "```", "${0}"],
    "description": "Inserts YAML code snippet"
  },
  "Rust Code Snippet": {
    "prefix": "rst",
    "scope": "markdown",
    "body": ["```rust", "${1}", "```", "${0}"],
    "description": "Inserts rust code snippet"
  },
  "Bash Code Snippet": {
    "prefix": "bsh",
    "scope": "markdown",
    "body": ["```bash", "${1}", "```", "${0}"],
    "description": "Inserts bash code snippet"
  },
  "Slides pre-processing": {
    "prefix": "slides_pre",
    "scope": "markdown",
    "body": ["```bash", "~~~${1:command}", "${0}", "~~~", "```"],
    "description": "Inserts slides pre-processing snippet"
  },
  "Parenthesis []": {
    "prefix": "par",
    "scope": "markdown",
    "body": ["[${1:$TM_SELECTED_TEXT}]"],
    "description": "Wrap selected in parenthesis"
  },
  "Bash Code Snippet with surround": {
    "prefix": "bshs",
    "scope": "markdown",
    "body": ["```bash", "${1:$TM_SELECTED_TEXT}", "```", "${0}"],
    "description": "Inserts bash code snippet"
  },
  "Document Front Matter": {
    "prefix": "docyaml",
    "body": [
      "---",
      "title: ${1:title}",
      "sidebar_label: ${2:label}",
      "tags:",
      "\t- ${3:tag1}",
      "\t- ${4:tag1}",
      "---\n${0}"
    ],
    "description": "Front matter snippet for docosaurus docs"
  },

  "Slides Theme": {
    "prefix": "theme",
    "body": [
      "---",
      "theme: ~/slides-themes/theme.json",
      "author: Piotr Zaniewski",
      "date: MMMM dd, YYYY",
      "paging: Slide %d / %d",
      "---"
    ],
    "description": "Insert header with slides theme"
  },
    "Insert 2x1 table": {
